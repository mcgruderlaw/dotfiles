! xterm -------------------------------------------------------

Xft.dpi:                   102

xterm*VT100.geometry:       80x25
XTerm*background:           #000000
!XTerm*background:           #FFFFCC
XTerm*foreground:           #bcbcbc
!XTerm*foreground:           #000000
XTerm*locale:               true
XTerm*scaleHeight:        1.1

xterm*termName:             xterm-256color
!,xterm*termName:             xterm
!xterm*termName:             terminator
xterm*faceName:             Inconsolata:style=Regular:size=10
!xterm*faceName:             Terminus:style=Regular:size=12
!xterm*faceName:           Terminus:style=Regular:size=10
!xterm*font:              -*-dina-medium-r-*-*-16-*-*-*-*-*-*-*
xterm*dynamicColors:      false
!xterm*utf8:               2
!xterm*eightBitInput:      true
xterm*saveLines:          512
!xterm*scrollKey:          true
xterm*scrollTtyOutput:    false
xterm*scrollBar:          false
xterm*rightScrollBar:     false
!xterm*jumpScroll:         true
!xterm*multiScroll:        true
xterm*toolBar:            false
!XTerm*reverseVideo:       on
